{
    "manifest_version": 3,
    "name": "ChatGPT Summary",
    "version": "1.0.0",
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "tabs",
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_title": "Wiki Summary",
      "default_icon": "icon.png",
      "default_popup": "popup/popup.html"    
    }
  }