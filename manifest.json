{
    "manifest_version": 3,
    "name": "ChatGPT Summary",
    "version": "1.0.0",
    "permissions": [
      "tabs",
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_title": "Wiki Summary",
      "default_icon": "assets/images/icon.png",
      "default_popup": "popup/popup.html"    
    },
    "background": {
      "service_worker": "service_worker.js",
      "type": "module"
    }
  }